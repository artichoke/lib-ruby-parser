--INPUT
foo, bar = 1, 2
--LOCATIONS
         ~ operator (root)
~~~~~~~~~~~~~~~ expression (root)
~~~~~~~~ expression (root -> lhs)
~~~ expression (root -> lhs -> mlhs_items -> 0)
     ~~~ expression (root -> lhs -> mlhs_items -> 1)
           ~~~~ expression (root -> rhs)
           ~ expression (root -> rhs -> elements -> 0)
              ~ expression (root -> rhs -> elements -> 1)
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "foo"),
    s(:lvasgn, "bar")),
  s(:array,
    s(:int, "1"),
    s(:int, "2")))
