--INPUT
m [], 1 do end
--LOCATIONS
        ~~ begin (root)
           ~~~ end (root)
~~~~~~~~~~~~~~ expression (root)
~ selector (root -> method_call)
~~~~~~~ expression (root -> method_call)
  ~ begin (root -> method_call -> args -> 0)
   ~ end (root -> method_call -> args -> 0)
  ~~ expression (root -> method_call -> args -> 0)
      ~ expression (root -> method_call -> args -> 1)
--AST
s(:block,
  s(:send, nil, "m",
    s(:array),
    s(:int, "1")), nil, nil)
