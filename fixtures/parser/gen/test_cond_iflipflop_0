--INPUT
if foo..bar; end
--LOCATIONS
~~ keyword (root)
             ~~~ end (root)
           ~ begin (root)
~~~~~~~~~~~~~~~~ expression (root)
      ~~ operator (root -> cond)
   ~~~~~~~~ expression (root -> cond)
   ~~~ expression (root -> cond -> left)
        ~~~ expression (root -> cond -> right)
--AST
s(:if,
  s(:iflipflop,
    s(:lvar, "foo"),
    s(:lvar, "bar")), nil, nil)
