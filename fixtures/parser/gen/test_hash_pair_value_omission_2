--INPUT
{BAR:}
--LOCATIONS
~ begin (root)
     ~ end (root)
~~~~~~ expression (root)
    ~ operator (root -> pairs -> 0)
 ~~~~ expression (root -> pairs -> 0)
 ~~~ expression (root -> pairs -> 0 -> key)
 ~~~ name (root -> pairs -> 0 -> value)
 ~~~ expression (root -> pairs -> 0 -> value)
--AST
s(:hash,
  s(:pair,
    s(:sym, "BAR"),
    s(:const, nil, "BAR")))
