--INPUT
until foo do meth end
--LOCATIONS
          ~~ begin (root)
                  ~~~ end (root)
~~~~~ keyword (root)
~~~~~~~~~~~~~~~~~~~~~ expression (root)
      ~~~ expression (root -> cond)
             ~~~~ selector (root -> body)
             ~~~~ expression (root -> body)
--AST
s(:until,
  s(:lvar, "foo"),
  s(:send, nil, "meth"))
