--INPUT
{ 'foo': 2 }
--LOCATIONS
~ begin (root)
           ~ end (root)
~~~~~~~~~~~~ expression (root)
       ~ operator (root -> pairs -> 0)
  ~~~~~~~~ expression (root -> pairs -> 0)
  ~ begin (root -> pairs -> 0 -> key)
      ~ end (root -> pairs -> 0 -> key)
  ~~~~~ expression (root -> pairs -> 0 -> key)
         ~ expression (root -> pairs -> 0 -> value)
--AST
s(:hash,
  s(:pair,
    s(:sym, "foo"),
    s(:int, "2")))
