--INPUT
"#@a #@@a #$a"
--LOCATIONS
~ begin ()
             ~ end ()
~~~~~~~~~~~~~~ expression ()
  ~~ name (part[0])
  ~~ expression (part[0])
    ~ expression (part[1])
      ~~~ name (part[2])
      ~~~ expression (part[2])
         ~ expression (part[3])
           ~~ name (part[4])
           ~~ expression (part[4])
--AST
s(:dstr,
  s(:ivar, "@a"),
  s(:str, " "),
  s(:cvar, "@@a"),
  s(:str, " "),
  s(:gvar, "$a"))
