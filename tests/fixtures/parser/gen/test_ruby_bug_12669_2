--INPUT
a = b += raise :x
--LOCATIONS
  ~ operator ()
~ name ()
~~~~~~~~~~~~~~~~~ expression ()
      ~~ operator (value)
    ~~~~~~~~~~~~~ expression (value)
    ~ name (value/recv)
    ~ expression (value/recv)
--AST
s(:lvasgn, "a",
  s(:op_asgn,
    s(:lvasgn, "b"), "+",
    s(:send, nil, "raise",
      s(:sym, "x"))))
