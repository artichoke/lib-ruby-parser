--INPUT
fun(f bar)
--LOCATIONS
   ~ begin ()
         ~ end ()
~~~ selector ()
~~~~~~~~~~ expression ()
    ~ selector (args[0])
    ~~~~~ expression (args[0])
      ~~~ name (args[0]/args[0])
      ~~~ expression (args[0]/args[0])
--AST
s(:send, nil, "fun",
  s(:send, nil, "f",
    s(:lvar, "bar")))
