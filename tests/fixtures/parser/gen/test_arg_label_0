--INPUT
def foo() a:b end
--LOCATIONS
~~~ keyword ()
    ~~~ name ()
              ~~~ end ()
~~~~~~~~~~~~~~~~~ expression ()
       ~ begin (args)
        ~ end (args)
       ~~ expression (args)
          ~ selector (body)
          ~~~ expression (body)
           ~ begin (body/args[0])
           ~~ expression (body/args[0])
--AST
s(:def, "foo",
  s(:args),
  s(:send, nil, "a",
    s(:sym, "b")))
