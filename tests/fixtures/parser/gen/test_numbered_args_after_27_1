--INPUT
m do _1 + _9 end
--LOCATIONS
  ~~ begin ()
             ~~~ end ()
~~~~~~~~~~~~~~~~ expression ()
~ selector (call)
~ expression (call)
        ~ selector (body)
     ~~~~~~~ expression (body)
     ~~ expression (body/recv)
          ~~ expression (body/arg[0])
--AST
s(:numblock,
  s(:send, nil, "m"), 9,
  s(:send,
    s(:lvar, "_1"), "+",
    s(:lvar, "_9")))
