--INPUT
f x: -> do meth do end end
--LOCATIONS
~ selector ()
~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
  ~~~~~~~~~~~~~~~~~~~~~~~~ expression (args[0])
   ~ operator (args[0]/pair[0])
  ~~~~~~~~~~~~~~~~~~~~~~~~ expression (args[0]/pair[0])
  ~ expression (args[0]/pair[0]/key)
        ~~ begin (args[0]/pair[0]/value)
                       ~~~ end (args[0]/pair[0]/value)
     ~~~~~~~~~~~~~~~~~~~~~ expression (args[0]/pair[0]/value)
     ~~ expression (args[0]/pair[0]/value/call)
                ~~ begin (args[0]/pair[0]/value/body)
                   ~~~ end (args[0]/pair[0]/value/body)
           ~~~~~~~~~~~ expression (args[0]/pair[0]/value/body)
           ~~~~ selector (args[0]/pair[0]/value/body/call)
           ~~~~ expression (args[0]/pair[0]/value/body/call)
--AST
s(:send, nil, "f",
  s(:hash,
    s(:pair,
      s(:sym, "x"),
      s(:block,
        s(:lambda), nil,
        s(:block,
          s(:send, nil, "meth"), nil, nil)))))
