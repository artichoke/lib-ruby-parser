--INPUT
p :foo, {** proc do end, b: proc do end}
--LOCATIONS
~ selector ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
  ~ begin (args[0])
  ~~~~ expression (args[0])
        ~ begin (args[1])
                                       ~ end (args[1])
        ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression (args[1])
         ~~ operator (args[1]/pair[0])
         ~~~~~~~~~~~~~~ expression (args[1]/pair[0])
                 ~~ begin (args[1]/pair[0]/value)
                    ~~~ end (args[1]/pair[0]/value)
            ~~~~~~~~~~~ expression (args[1]/pair[0]/value)
            ~~~~ selector (args[1]/pair[0]/value/call)
            ~~~~ expression (args[1]/pair[0]/value/call)
                          ~ operator (args[1]/pair[1])
                         ~~~~~~~~~~~~~~ expression (args[1]/pair[1])
                         ~ expression (args[1]/pair[1]/key)
                                 ~~ begin (args[1]/pair[1]/value)
                                    ~~~ end (args[1]/pair[1]/value)
                            ~~~~~~~~~~~ expression (args[1]/pair[1]/value)
                            ~~~~ selector (args[1]/pair[1]/value/call)
                            ~~~~ expression (args[1]/pair[1]/value/call)
--AST
s(:send, nil, "p",
  s(:sym, "foo"),
  s(:hash,
    s(:kwsplat,
      s(:block,
        s(:send, nil, "proc"), nil, nil)),
    s(:pair,
      s(:sym, "b"),
      s(:block,
        s(:send, nil, "proc"), nil, nil))))
