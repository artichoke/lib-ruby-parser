--INPUT
f{ |**baz, &b| }
--LOCATIONS
 ~ begin (root)
               ~ end (root)
~~~~~~~~~~~~~~~~ expression (root)
~ selector (root -> method_call)
~ expression (root -> method_call)
   ~ begin (root -> args)
             ~ end (root -> args)
   ~~~~~~~~~~~ expression (root -> args)
      ~~~ name (root -> args -> arglist -> 0)
    ~~~~~ expression (root -> args -> arglist -> 0)
            ~ name (root -> args -> arglist -> 1)
           ~~ expression (root -> args -> arglist -> 1)
--AST
s(:block,
  s(:send, nil, "f"),
  s(:args,
    s(:kwrestarg, "baz"),
    s(:blockarg, "b")), nil)
