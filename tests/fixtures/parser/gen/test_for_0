--INPUT
for a in foo do p a; end
--LOCATIONS
~~~ keyword ()
                     ~~~ end ()
             ~~ begin ()
~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
    ~ name (iterator)
    ~ expression (iterator)
         ~~~ name (iteratee)
         ~~~ expression (iteratee)
                ~ selector (body)
                ~~~ expression (body)
                  ~ name (body/arg[0])
                  ~ expression (body/arg[0])
--AST
s(:for,
  s(:lvasgn, "a"),
  s(:lvar, "foo"),
  s(:send, nil, "p",
    s(:lvar, "a")))
