--INPUT
def inc(x) = x + 1
--LOCATIONS
~~~ keyword ()
    ~~~ name ()
           ~ assignment ()
~~~~~~~~~~~~~~~~~~ expression ()
       ~ begin (args)
         ~ end (args)
       ~~~ expression (args)
        ~ name (args/arg[0])
        ~ expression (args/arg[0])
               ~ selector (body)
             ~~~~~ expression (body)
             ~ name (body/recv)
             ~ expression (body/recv)
                 ~ expression (body/args[0])
--AST
s(:def, "inc",
  s(:args,
    s(:arg, "x")),
  s(:send,
    s(:lvar, "x"), "+",
    s(:int, "1")))
