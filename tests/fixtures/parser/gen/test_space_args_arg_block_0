--INPUT
fun (1) {}
--LOCATIONS
        ~ begin ()
         ~ end ()
~~~~~~~~~~ expression ()
~~~ selector (call)
~~~~~~~ expression (call)
    ~ begin (call/arg[0])
      ~ end (call/arg[0])
    ~~~ expression (call/arg[0])
     ~ expression (call/arg[0]/body)
--AST
s(:block,
  s(:send, nil, "fun",
    s(:begin,
      s(:int, "1"))), nil, nil)
