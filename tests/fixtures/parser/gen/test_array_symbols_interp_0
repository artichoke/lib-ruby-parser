--INPUT
%I[foo #{bar}]
--LOCATIONS
~~~ begin ()
             ~ end ()
~~~~~~~~~~~~~~ expression ()
   ~~~ expression (element[0])
       ~~~~~~ expression (element[1])
       ~~ begin (element[1]/part[0])
            ~ end (element[1]/part[0])
       ~~~~~~ expression (element[1]/part[0])
         ~~~ name (element[1]/part[0]/stmt[0])
         ~~~ expression (element[1]/part[0]/stmt[0])
--AST
s(:array,
  s(:sym, "foo"),
  s(:dsym,
    s(:begin,
      s(:lvar, "bar"))))
