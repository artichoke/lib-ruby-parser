--INPUT
fun(*bar, &baz)
--LOCATIONS
   ~ begin ()
              ~ end ()
~~~ selector ()
~~~~~~~~~~~~~~~ expression ()
    ~ operator (args[0])
    ~~~~ expression (args[0])
     ~~~ name (args[0]/value)
     ~~~ expression (args[0]/value)
          ~ operator (args[1])
          ~~~~ expression (args[1])
           ~~~ name (args[1]/value)
           ~~~ expression (args[1]/value)
--AST
s(:send, nil, "fun",
  s(:splat,
    s(:lvar, "bar")),
  s(:block_pass,
    s(:lvar, "baz")))
