--INPUT
let () { m(a) do; end }
--LOCATIONS
       ~ begin ()
                      ~ end ()
~~~~~~~~~~~~~~~~~~~~~~~ expression ()
~~~ selector (call)
~~~~~~ expression (call)
    ~ begin (call/args[0])
     ~ end (call/args[0])
    ~~ expression (call/args[0])
              ~~ begin (body)
                  ~~~ end (body)
         ~~~~~~~~~~~~ expression (body)
          ~ begin (body/call)
            ~ end (body/call)
         ~ selector (body/call)
         ~~~~ expression (body/call)
           ~ selector (body/call/args[0])
           ~ expression (body/call/args[0])
--AST
s(:block,
  s(:send, nil, "let",
    s(:begin)), nil,
  s(:block,
    s(:send, nil, "m",
      s(:send, nil, "a")), nil, nil))
