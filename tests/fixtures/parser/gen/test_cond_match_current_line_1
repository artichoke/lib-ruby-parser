--INPUT
!/wat/
--LOCATIONS
~ selector ()
~~~~~~ expression ()
 ~~~~~ expression (recv)
 ~ begin (recv/value)
     ~ end (recv/value)
 ~~~~~ expression (recv/value)
  ~~~ expression (recv/value/part[0])
       expression (recv/value/opts)
--AST
s(:send,
  s(:match_current_line,
    s(:regexp,
      s(:str, "wat"),
      s(:regopt))), "!")
