--INPUT
case foo; in ^foo then nil; end
--LOCATIONS
~~~~ keyword ()
                            ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
     ~~~ name (expr)
     ~~~ expression (expr)
                  ~~~~ begin (in_body[0])
          ~~ keyword (in_body[0])
          ~~~~~~~~~~~~~~~~ expression (in_body[0])
             ~ selector (in_body[0]/pattern)
             ~~~~ expression (in_body[0]/pattern)
              ~~~ name (in_body[0]/pattern/value)
              ~~~ expression (in_body[0]/pattern/value)
                       ~~~ expression (in_body[0]/body)
--AST
s(:case_match,
  s(:lvar, "foo"),
  s(:in_pattern,
    s(:pin,
      s(:lvar, "foo")), nil,
    s(:nil)), nil)
