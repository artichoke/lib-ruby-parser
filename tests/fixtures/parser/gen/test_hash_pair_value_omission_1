--INPUT
{puts:}
--LOCATIONS
~ begin (root)
      ~ end (root)
~~~~~~~ expression (root)
     ~ operator (root -> pairs -> 0)
 ~~~~~ expression (root -> pairs -> 0)
 ~~~~ expression (root -> pairs -> 0 -> key)
 ~~~~ selector (root -> pairs -> 0 -> value)
 ~~~~ expression (root -> pairs -> 0 -> value)
--AST
s(:hash,
  s(:pair,
    s(:sym, "puts"),
    s(:send, nil, "puts")))
