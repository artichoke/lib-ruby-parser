--INPUT
begin meth end while foo
--LOCATIONS
               ~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
                     ~~~ name (cond)
                     ~~~ expression (cond)
~~~~~ begin (body)
           ~~~ end (body)
~~~~~~~~~~~~~~ expression (body)
      ~~~~ selector (body/stmt[0])
      ~~~~ expression (body/stmt[0])
--AST
s(:while_post,
  s(:lvar, "foo"),
  s(:kwbegin,
    s(:send, nil, "meth")))
