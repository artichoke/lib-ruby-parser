--INPUT
foo.C += raise(bar) rescue nil
--LOCATIONS
      ~~ operator (root)
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression (root)
    ~ selector (root -> recv)
~~~~~ expression (root -> recv)
~~~ expression (root -> recv -> recv)
--AST
s(:op_asgn,
  s(:send,
    s(:lvar, "foo"), "C"), "+",
  s(:rescue,
    s(:send, nil, "raise",
      s(:lvar, "bar")),
    s(:resbody, nil, nil,
      s(:nil)), nil))
