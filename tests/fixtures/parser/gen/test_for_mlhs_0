--INPUT
for a, b in foo; p a, b; end
--LOCATIONS
~~~ keyword ()
                         ~~~ end ()
               ~ begin ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
    ~~~~ expression (iterator)
    ~ name (iterator/item[0])
    ~ expression (iterator/item[0])
       ~ name (iterator/item[1])
       ~ expression (iterator/item[1])
            ~~~ name (iteratee)
            ~~~ expression (iteratee)
                 ~ selector (body)
                 ~~~~~~ expression (body)
                   ~ name (body/args[0])
                   ~ expression (body/args[0])
                      ~ name (body/args[1])
                      ~ expression (body/args[1])
--AST
s(:for,
  s(:mlhs,
    s(:lvasgn, "a"),
    s(:lvasgn, "b")),
  s(:lvar, "foo"),
  s(:send, nil, "p",
    s(:lvar, "a"),
    s(:lvar, "b")))
