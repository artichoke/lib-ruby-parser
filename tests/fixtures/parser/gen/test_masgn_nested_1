--INPUT
((b, )) = foo
--LOCATIONS
        ~ operator ()
~~~~~~~~~~~~~ expression ()
~ begin (lhs)
      ~ end (lhs)
~~~~~~~ expression (lhs)
  ~ name (lhs/item[0])
  ~ expression (lhs/item[0])
          ~~~ name (rhs)
          ~~~ expression (rhs)
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "b")),
  s(:lvar, "foo"))
