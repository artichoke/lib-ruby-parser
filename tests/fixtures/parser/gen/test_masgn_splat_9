--INPUT
*, c, d = bar
--LOCATIONS
        ~ operator ()
~~~~~~~~~~~~~ expression ()
~~~~~~~ expression (lhs)
~ operator (lhs/item[0])
~ expression (lhs/item[0])
   ~ name (lhs/item[1])
   ~ expression (lhs/item[1])
      ~ name (lhs/item[2])
      ~ expression (lhs/item[2])
          ~~~ name (rhs)
          ~~~ expression (rhs)
--AST
s(:masgn,
  s(:mlhs,
    s(:splat),
    s(:lvasgn, "c"),
    s(:lvasgn, "d")),
  s(:lvar, "bar"))
