--INPUT
-> do _1 + _9 end
--LOCATIONS
   ~~ begin ()
              ~~~ end ()
~~~~~~~~~~~~~~~~~ expression ()
~~ expression (call)
         ~ selector (body)
      ~~~~~~~ expression (body)
      ~~ name (body/recv)
      ~~ expression (body/recv)
           ~~ name (body/args[0])
           ~~ expression (body/args[0])
--AST
s(:numblock,
  s(:lambda), 9,
  s(:send,
    s(:lvar, "_1"), "+",
    s(:lvar, "_9")))
