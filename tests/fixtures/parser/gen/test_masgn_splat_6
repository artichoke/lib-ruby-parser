--INPUT
*b = bar
--LOCATIONS
   ~ operator (root)
~~~~~~~~ expression (root)
~~ expression (root -> lhs)
~ operator (root -> lhs -> mlhs_items -> 0)
~~ expression (root -> lhs -> mlhs_items -> 0)
 ~ expression (root -> lhs -> mlhs_items -> 0 -> value)
     ~~~ expression (root -> rhs)
--AST
s(:masgn,
  s(:mlhs,
    s(:splat,
      s(:lvasgn, "b"))),
  s(:lvar, "bar"))
