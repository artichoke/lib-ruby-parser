--INPUT
class << foo; _1; end
--LOCATIONS
~~~~~ keyword ()
      ~~ operator ()
                  ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~ expression ()
         ~~~ name (expr)
         ~~~ expression (expr)
              ~~ selector (body)
              ~~ expression (body)
--AST
s(:sclass,
  s(:lvar, "foo"),
  s(:send, nil, "_1"))
