--INPUT
p ->() do a() do end end
--LOCATIONS
~ selector ()
~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
       ~~ begin (args[0])
                     ~~~ end (args[0])
  ~~~~~~~~~~~~~~~~~~~~~~ expression (args[0])
  ~~ expression (args[0]/call)
    ~ begin (args[0]/args)
     ~ end (args[0]/args)
    ~~ expression (args[0]/args)
              ~~ begin (args[0]/body)
                 ~~~ end (args[0]/body)
          ~~~~~~~~~~ expression (args[0]/body)
           ~ begin (args[0]/body/call)
            ~ end (args[0]/body/call)
          ~ selector (args[0]/body/call)
          ~~~ expression (args[0]/body/call)
--AST
s(:send, nil, "p",
  s(:block,
    s(:lambda),
    s(:args),
    s(:block,
      s(:send, nil, "a"), nil, nil)))
