--INPUT
-> (a) { }
--LOCATIONS
       ~ begin (root)
         ~ end (root)
~~~~~~~~~~ expression (root)
~~ expression (root -> method_call)
   ~ begin (root -> args)
     ~ end (root -> args)
   ~~~ expression (root -> args)
    ~ expression (root -> args -> arglist -> 0)
--AST
s(:block,
  s(:lambda),
  s(:args,
    s(:arg, "a")), nil)
