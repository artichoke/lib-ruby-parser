--INPUT
a = b = raise :x
--LOCATIONS
  ~ operator ()
~ name ()
~~~~~~~~~~~~~~~~ expression ()
      ~ operator (value)
    ~ name (value)
    ~~~~~~~~~~~~ expression (value)
        ~~~~~ selector (value/value)
        ~~~~~~~~ expression (value/value)
              ~ begin (value/value/args[0])
              ~~ expression (value/value/args[0])
--AST
s(:lvasgn, "a",
  s(:lvasgn, "b",
    s(:send, nil, "raise",
      s(:sym, "x"))))
