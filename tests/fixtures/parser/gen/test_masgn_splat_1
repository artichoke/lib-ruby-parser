--INPUT
a, b = *foo, bar
--LOCATIONS
     ~ operator ()
~~~~~~~~~~~~~~~~ expression ()
~~~~ expression (lhs)
~ name (lhs/item[0])
~ expression (lhs/item[0])
   ~ name (lhs/item[1])
   ~ expression (lhs/item[1])
       ~~~~~~~~~ expression (rhs)
       ~ operator (rhs/element[0])
       ~~~~ expression (rhs/element[0])
        ~~~ name (rhs/element[0]/value)
        ~~~ expression (rhs/element[0]/value)
             ~~~ name (rhs/element[1])
             ~~~ expression (rhs/element[1])
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "a"),
    s(:lvasgn, "b")),
  s(:array,
    s(:splat,
      s(:lvar, "foo")),
    s(:lvar, "bar")))
