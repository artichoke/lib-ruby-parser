--INPUT
case foo; in 1...2 then true; end
--LOCATIONS
~~~~ keyword ()
                              ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
     ~~~ expression (expr)
                   ~~~~ begin (in_body[0])
          ~~ keyword (in_body[0])
          ~~~~~~~~~~~~~~~~~~ expression (in_body[0])
              ~~~ operator (in_body[0]/pattern)
             ~~~~~ expression (in_body[0]/pattern)
             ~ expression (in_body[0]/pattern/left)
                 ~ expression (in_body[0]/pattern/right)
                        ~~~~ expression (in_body[0]/body)
--AST
s(:case_match,
  s(:lvar, "foo"),
  s(:in_pattern,
    s(:erange,
      s(:int, "1"),
      s(:int, "2")), nil,
    s(:true)), nil)
