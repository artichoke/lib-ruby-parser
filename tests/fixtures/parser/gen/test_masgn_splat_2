--INPUT
a, *b = bar
--LOCATIONS
      ~ operator ()
~~~~~~~~~~~ expression ()
~~~~~ expression (lhs)
~ name (lhs/item[0])
~ expression (lhs/item[0])
   ~ operator (lhs/item[1])
   ~~ expression (lhs/item[1])
    ~ name (lhs/item[1]/value)
    ~ expression (lhs/item[1]/value)
        ~~~ name (rhs)
        ~~~ expression (rhs)
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "a"),
    s(:splat,
      s(:lvasgn, "b"))),
  s(:lvar, "bar"))
