--INPUT
begin; meth; rescue Exception; bar; end
--LOCATIONS
~~~~~ begin ()
                                    ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression (stmt[0])
       ~~~~ selector (stmt[0]/body)
       ~~~~ expression (stmt[0]/body)
             ~~~~~~ keyword (stmt[0]/rescue_body[0])
                             ~ begin (stmt[0]/rescue_body[0])
             ~~~~~~~~~~~~~~~~~~~~~ expression (stmt[0]/rescue_body[0])
                    ~~~~~~~~~ expression (stmt[0]/rescue_body[0]/exc_list)
                    ~~~~~~~~~ name (stmt[0]/rescue_body[0]/exc_list/item[0])
                    ~~~~~~~~~ expression (stmt[0]/rescue_body[0]/exc_list/item[0])
                               ~~~ name (stmt[0]/rescue_body[0]/body)
                               ~~~ expression (stmt[0]/rescue_body[0]/body)
--AST
s(:kwbegin,
  s(:rescue,
    s(:send, nil, "meth"),
    s(:resbody,
      s(:array,
        s(:const, nil, "Exception")), nil,
      s(:lvar, "bar")), nil))
