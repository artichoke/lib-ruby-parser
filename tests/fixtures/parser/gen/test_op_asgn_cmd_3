--INPUT
foo::A += m foo
--LOCATIONS
     ~ name ()
   ~~ double_colon ()
~~~~~~~~~~~~~~~ expression ()
     ~ name (recv)
   ~~ double_colon (recv)
~~~~~~ expression (recv)
~~~ name (recv/scope)
~~~ expression (recv/scope)
--AST
s(:op_asgn,
  s(:casgn,
    s(:lvar, "foo"), "A"), "+",
  s(:send, nil, "m",
    s(:lvar, "foo")))
