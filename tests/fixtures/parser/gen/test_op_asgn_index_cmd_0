--INPUT
foo[0, 1] += m foo
--LOCATIONS
   ~ begin ()
        ~ end ()
          ~~ operator ()
~~~~~~~~~~~~~~~~~~ expression ()
   ~ begin (recv)
        ~ end (recv)
~~~~~~~~~ expression (recv)
~~~ name (recv/recv)
~~~ expression (recv/recv)
    ~ expression (recv/index[0])
       ~ expression (recv/value)
--AST
s(:op_asgn,
  s(:indexasgn,
    s(:lvar, "foo"),
    s(:int, "0"),
    s(:int, "1")), "+",
  s(:send, nil, "m",
    s(:lvar, "foo")))
