--INPUT
m { _1 + _9 }
--LOCATIONS
  ~ begin ()
            ~ end ()
~~~~~~~~~~~~~ expression ()
~ selector (call)
~ expression (call)
       ~ selector (body)
    ~~~~~~~ expression (body)
    ~~ name (body/recv)
    ~~ expression (body/recv)
         ~~ name (body/args[0])
         ~~ expression (body/args[0])
--AST
s(:numblock,
  s(:send, nil, "m"), 9,
  s(:send,
    s(:lvar, "_1"), "+",
    s(:lvar, "_9")))
