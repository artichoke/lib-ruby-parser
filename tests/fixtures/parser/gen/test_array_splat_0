--INPUT
[1, *foo, 2]
--LOCATIONS
~ begin ()
           ~ end ()
~~~~~~~~~~~~ expression ()
 ~ expression (element[0])
    ~ operator (element[1])
    ~~~~ expression (element[1])
     ~~~ name (element[1]/value)
     ~~~ expression (element[1]/value)
          ~ expression (element[2])
--AST
s(:array,
  s(:int, "1"),
  s(:splat,
    s(:lvar, "foo")),
  s(:int, "2"))
