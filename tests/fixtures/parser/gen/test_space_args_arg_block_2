--INPUT
foo::fun (1) {}
--LOCATIONS
             ~ begin ()
              ~ end ()
~~~~~~~~~~~~~~~ expression ()
     ~~~ selector (call)
~~~~~~~~~~~~ expression (call)
~~~ name (call/recv)
~~~ expression (call/recv)
         ~ begin (call/args[0])
           ~ end (call/args[0])
         ~~~ expression (call/args[0])
          ~ expression (call/args[0]/stmt[0])
--AST
s(:block,
  s(:send,
    s(:lvar, "foo"), "fun",
    s(:begin,
      s(:int, "1"))), nil, nil)
