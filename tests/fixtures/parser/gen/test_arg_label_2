--INPUT
f { || a:b }
--LOCATIONS
  ~ begin ()
           ~ end ()
~~~~~~~~~~~~ expression ()
~ selector (call)
~ expression (call)
    ~ begin (args)
     ~ end (args)
    ~~ expression (args)
       ~ selector (body)
       ~~~ expression (body)
        ~ begin (body/args[0])
        ~~ expression (body/args[0])
--AST
s(:block,
  s(:send, nil, "f"),
  s(:args),
  s(:send, nil, "a",
    s(:sym, "b")))
