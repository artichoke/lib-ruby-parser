--INPUT
foo, bar, baz = 1, 2
--LOCATIONS
              ~ operator ()
~~~~~~~~~~~~~~~~~~~~ expression ()
~~~~~~~~~~~~~ expression (lhs)
~~~ name (lhs/item[0])
~~~ expression (lhs/item[0])
     ~~~ name (lhs/item[1])
     ~~~ expression (lhs/item[1])
          ~~~ name (lhs/item[2])
          ~~~ expression (lhs/item[2])
                ~~~~ expression (rhs)
                ~ expression (rhs/item[0])
                   ~ expression (rhs/item[1])
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "foo"),
    s(:lvasgn, "bar"),
    s(:lvasgn, "baz")),
  s(:array,
    s(:int, "1"),
    s(:int, "2")))
