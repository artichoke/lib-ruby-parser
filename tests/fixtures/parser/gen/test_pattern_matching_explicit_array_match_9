--INPUT
case foo; in [*, x] then true; end
--LOCATIONS
~~~~ keyword ()
                               ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
     ~~~ name (expr)
     ~~~ expression (expr)
                    ~~~~ begin (in_body[0])
          ~~ keyword (in_body[0])
          ~~~~~~~~~~~~~~~~~~~ expression (in_body[0])
             ~ begin (in_body[0]/pattern)
                  ~ end (in_body[0]/pattern)
             ~~~~~~ expression (in_body[0]/pattern)
              ~ operator (in_body[0]/pattern/item[0])
              ~ expression (in_body[0]/pattern/item[0])
                 ~ name (in_body[0]/pattern/item[1])
                 ~ expression (in_body[0]/pattern/item[1])
                         ~~~~ expression (in_body[0]/body)
--AST
s(:case_match,
  s(:lvar, "foo"),
  s(:in_pattern,
    s(:array_pattern,
      s(:match_rest),
      s(:match_var, "x")), nil,
    s(:true)), nil)
