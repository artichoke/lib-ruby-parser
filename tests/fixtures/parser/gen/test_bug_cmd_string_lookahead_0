--INPUT
desc "foo" do end
--LOCATIONS
           ~~ begin ()
              ~~~ end ()
~~~~~~~~~~~~~~~~~ expression ()
~~~~ selector (call)
~~~~~~~~~~ expression (call)
     ~ begin (call/args[0])
         ~ end (call/args[0])
     ~~~~~ expression (call/args[0])
--AST
s(:block,
  s(:send, nil, "desc",
    s(:str, "foo")), nil, nil)
