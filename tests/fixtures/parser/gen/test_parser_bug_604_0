--INPUT
m a + b do end
--LOCATIONS
        ~~ begin ()
           ~~~ end ()
~~~~~~~~~~~~~~ expression ()
~ selector (call)
~~~~~~~ expression (call)
    ~ selector (call/args[0])
  ~~~~~ expression (call/args[0])
  ~ selector (call/args[0]/recv)
  ~ expression (call/args[0]/recv)
      ~ selector (call/args[0]/args[0])
      ~ expression (call/args[0]/args[0])
--AST
s(:block,
  s(:send, nil, "m",
    s(:send,
      s(:send, nil, "a"), "+",
      s(:send, nil, "b"))), nil, nil)
