--INPUT
foo = bar = m foo
--LOCATIONS
    ~ operator ()
~~~ name ()
~~~~~~~~~~~~~~~~~ expression ()
          ~ operator (value)
      ~~~ name (value)
      ~~~~~~~~~~~ expression (value)
            ~ selector (value/value)
            ~~~~~ expression (value/value)
              ~~~ name (value/value/args[0])
              ~~~ expression (value/value/args[0])
--AST
s(:lvasgn, "foo",
  s(:lvasgn, "bar",
    s(:send, nil, "m",
      s(:lvar, "foo"))))
