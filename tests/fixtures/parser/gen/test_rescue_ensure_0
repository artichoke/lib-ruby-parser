--INPUT
begin; meth; rescue; baz; ensure; bar; end
--LOCATIONS
~~~~~ begin ()
                                       ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
                          ~~~~~~ keyword (body)
       ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression (body)
       ~~~~~~~~~~~~~~~~~ expression (body/body)
       ~~~~ selector (body/body/body)
       ~~~~ expression (body/body/body)
             ~~~~~~ keyword (body/body/rescue_body[0])
                   ~ begin (body/body/rescue_body[0])
             ~~~~~~~~~~~ expression (body/body/rescue_body[0])
                     ~~~ name (body/body/rescue_body[0]/body)
                     ~~~ expression (body/body/rescue_body[0]/body)
                                  ~~~ name (body/ensure_body)
                                  ~~~ expression (body/ensure_body)
--AST
s(:kwbegin,
  s(:ensure,
    s(:rescue,
      s(:send, nil, "meth"),
      s(:resbody, nil, nil,
        s(:lvar, "baz")), nil),
    s(:lvar, "bar")))
