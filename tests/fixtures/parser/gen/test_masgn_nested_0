--INPUT
a, (b, c) = foo
--LOCATIONS
          ~ operator ()
~~~~~~~~~~~~~~~ expression ()
~~~~~~~~~ expression (lhs)
~ name (lhs/item[0])
~ expression (lhs/item[0])
   ~ begin (lhs/item[1])
        ~ end (lhs/item[1])
   ~~~~~~ expression (lhs/item[1])
    ~ name (lhs/item[1]/item[0])
    ~ expression (lhs/item[1]/item[0])
       ~ name (lhs/item[1]/item[1])
       ~ expression (lhs/item[1]/item[1])
            ~~~ name (rhs)
            ~~~ expression (rhs)
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "a"),
    s(:mlhs,
      s(:lvasgn, "b"),
      s(:lvasgn, "c"))),
  s(:lvar, "foo"))
