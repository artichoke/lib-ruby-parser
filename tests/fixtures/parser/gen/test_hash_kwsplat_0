--INPUT
{ foo: 2, **bar }
--LOCATIONS
~ begin ()
                ~ end ()
~~~~~~~~~~~~~~~~~ expression ()
     ~ operator (pair[0])
  ~~~~~~ expression (pair[0])
  ~~~ expression (pair[0]/key)
       ~ expression (pair[0]/value)
          ~~ operator (pair[1])
          ~~~~~ expression (pair[1])
            ~~~ name (pair[1]/value)
            ~~~ expression (pair[1]/value)
--AST
s(:hash,
  s(:pair,
    s(:sym, "foo"),
    s(:int, "2")),
  s(:kwsplat,
    s(:lvar, "bar")))
