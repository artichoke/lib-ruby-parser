--INPUT
m ->(a: ->{_1}) {a}
--LOCATIONS
~ selector ()
~~~~~~~~~~~~~~~~~~~ expression ()
                ~ begin (args[0])
                  ~ end (args[0])
  ~~~~~~~~~~~~~~~~~ expression (args[0])
  ~~ expression (args[0]/call)
    ~ begin (args[0]/args)
              ~ end (args[0]/args)
    ~~~~~~~~~~~ expression (args[0]/args)
     ~ name (args[0]/args/arg[0])
     ~~~~~~~~~ expression (args[0]/args/arg[0])
          ~ begin (args[0]/args/arg[0]/default)
             ~ end (args[0]/args/arg[0]/default)
        ~~~~~~ expression (args[0]/args/arg[0]/default)
        ~~ expression (args[0]/args/arg[0]/default/call)
           ~~ name (args[0]/args/arg[0]/default/body)
           ~~ expression (args[0]/args/arg[0]/default/body)
                 ~ name (args[0]/body)
                 ~ expression (args[0]/body)
--AST
s(:send, nil, "m",
  s(:block,
    s(:lambda),
    s(:args,
      s(:kwoptarg, "a",
        s(:numblock,
          s(:lambda), 1,
          s(:lvar, "_1")))),
    s(:lvar, "a")))
