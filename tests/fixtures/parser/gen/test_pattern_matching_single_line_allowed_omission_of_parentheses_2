--INPUT
[1, 2] in a, b; a
--LOCATIONS
~~~~~~~~~~~~~~~~~ expression (root)
       ~~ operator (root -> stmts -> 0)
~~~~~~~~~~~~~~ expression (root -> stmts -> 0)
~ begin (root -> stmts -> 0 -> value)
     ~ end (root -> stmts -> 0 -> value)
~~~~~~ expression (root -> stmts -> 0 -> value)
 ~ expression (root -> stmts -> 0 -> value -> elements -> 0)
    ~ expression (root -> stmts -> 0 -> value -> elements -> 1)
          ~~~~ expression (root -> stmts -> 0 -> pattern)
          ~ name (root -> stmts -> 0 -> pattern -> elements -> 0)
          ~ expression (root -> stmts -> 0 -> pattern -> elements -> 0)
             ~ name (root -> stmts -> 0 -> pattern -> elements -> 1)
             ~ expression (root -> stmts -> 0 -> pattern -> elements -> 1)
                ~ expression (root -> stmts -> 1)
--AST
s(:begin,
  s(:match_pattern_p,
    s(:array,
      s(:int, "1"),
      s(:int, "2")),
    s(:array_pattern,
      s(:match_var, "a"),
      s(:match_var, "b"))),
  s(:lvar, "a"))
