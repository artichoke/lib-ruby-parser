--INPUT
begin; meth; ensure; bar; end
--LOCATIONS
~~~~~ begin ()
                          ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
             ~~~~~~ keyword (stmt[0])
       ~~~~~~~~~~~~~~~~~ expression (stmt[0])
       ~~~~ selector (stmt[0]/body)
       ~~~~ expression (stmt[0]/body)
                     ~~~ name (stmt[0]/ensure_body)
                     ~~~ expression (stmt[0]/ensure_body)
--AST
s(:kwbegin,
  s(:ensure,
    s(:send, nil, "meth"),
    s(:lvar, "bar")))
