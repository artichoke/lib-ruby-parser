--INPUT
meth 1 do end::fun(bar)
--LOCATIONS
                  ~ begin (root)
                      ~ end (root)
               ~~~ selector (root)
~~~~~~~~~~~~~~~~~~~~~~~ expression (root)
       ~~ begin (root -> recv)
          ~~~ end (root -> recv)
~~~~~~~~~~~~~ expression (root -> recv)
~~~~ selector (root -> recv -> method_call)
~~~~~~ expression (root -> recv -> method_call)
     ~ expression (root -> recv -> method_call -> args -> 0)
                   ~~~ expression (root -> args -> 0)
--AST
s(:send,
  s(:block,
    s(:send, nil, "meth",
      s(:int, "1")), nil, nil), "fun",
  s(:lvar, "bar"))
