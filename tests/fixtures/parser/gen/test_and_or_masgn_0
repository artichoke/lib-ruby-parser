--INPUT
foo && (a, b = bar)
--LOCATIONS
    ~~ operator ()
~~~~~~~~~~~~~~~~~~~ expression ()
~~~ name (lhs)
~~~ expression (lhs)
       ~ begin (rhs)
                  ~ end (rhs)
       ~~~~~~~~~~~~ expression (rhs)
             ~ operator (rhs/stmt[0])
        ~~~~~~~~~~ expression (rhs/stmt[0])
        ~~~~ expression (rhs/stmt[0]/lhs)
        ~ name (rhs/stmt[0]/lhs/item[0])
        ~ expression (rhs/stmt[0]/lhs/item[0])
           ~ name (rhs/stmt[0]/lhs/item[1])
           ~ expression (rhs/stmt[0]/lhs/item[1])
               ~~~ name (rhs/stmt[0]/rhs)
               ~~~ expression (rhs/stmt[0]/rhs)
--AST
s(:and,
  s(:lvar, "foo"),
  s(:begin,
    s(:masgn,
      s(:mlhs,
        s(:lvasgn, "a"),
        s(:lvasgn, "b")),
      s(:lvar, "bar"))))
