--INPUT
/\xa8/n =~ ""
--LOCATIONS
        ~~ selector ()
~~~~~~~~~~~~~ expression ()
~ begin (re)
     ~ end (re)
~~~~~~~ expression (re)
 ~~~~ expression (re/src)
      ~ expression (re/opts)
           ~ begin (value)
            ~ end (value)
           ~~ expression (value)
--AST
s(:match_with_lvasgn,
  s(:regexp,
    s(:str, "\\xa8"),
    s(:regopt, "n")),
  s(:str, ""))
