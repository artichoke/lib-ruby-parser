--INPUT
self::a, foo = foo
--LOCATIONS
             ~ operator ()
~~~~~~~~~~~~~~~~~~ expression ()
~~~~~~~~~~~~ expression (lhs)
      ~ selector (lhs/item[0])
~~~~~~~ expression (lhs/item[0])
~~~~ expression (lhs/item[0]/recv)
         ~~~ name (lhs/item[1])
         ~~~ expression (lhs/item[1])
               ~~~ name (rhs)
               ~~~ expression (rhs)
--AST
s(:masgn,
  s(:mlhs,
    s(:send,
      s(:self), "a="),
    s(:lvasgn, "foo")),
  s(:lvar, "foo"))
