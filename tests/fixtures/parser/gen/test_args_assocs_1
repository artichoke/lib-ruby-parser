--INPUT
fun(:foo => 1, &baz)
--LOCATIONS
   ~ begin ()
                   ~ end ()
~~~ selector ()
~~~~~~~~~~~~~~~~~~~~ expression ()
    ~~~~~~~~~ expression (args[0])
         ~~ operator (args[0]/pair[0])
    ~~~~~~~~~ expression (args[0]/pair[0])
    ~ begin (args[0]/pair[0]/key)
    ~~~~ expression (args[0]/pair[0]/key)
            ~ expression (args[0]/pair[0]/value)
               ~ operator (args[1])
               ~~~~ expression (args[1])
                ~~~ name (args[1]/value)
                ~~~ expression (args[1]/value)
--AST
s(:send, nil, "fun",
  s(:hash,
    s(:pair,
      s(:sym, "foo"),
      s(:int, "1"))),
  s(:block_pass,
    s(:lvar, "baz")))
