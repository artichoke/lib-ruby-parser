--INPUT
meth 1 do end.fun {}
--LOCATIONS
                  ~ begin ()
                   ~ end ()
~~~~~~~~~~~~~~~~~~~~ expression ()
              ~~~ selector (call)
~~~~~~~~~~~~~~~~~ expression (call)
       ~~ begin (call/recv)
          ~~~ end (call/recv)
~~~~~~~~~~~~~ expression (call/recv)
~~~~ selector (call/recv/call)
~~~~~~ expression (call/recv/call)
     ~ expression (call/recv/call/args[0])
--AST
s(:block,
  s(:send,
    s(:block,
      s(:send, nil, "meth",
        s(:int, "1")), nil, nil), "fun"), nil, nil)
