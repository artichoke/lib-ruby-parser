--INPUT
%I[foo#{bar}]
--LOCATIONS
~~~ begin ()
            ~ end ()
~~~~~~~~~~~~~ expression ()
   ~~~~~~~~~ expression (element[0])
   ~~~ expression (element[0]/part[0])
      ~~ begin (element[0]/part[1])
           ~ end (element[0]/part[1])
      ~~~~~~ expression (element[0]/part[1])
        ~~~ name (element[0]/part[1]/stmt[0])
        ~~~ expression (element[0]/part[1]/stmt[0])
--AST
s(:array,
  s(:dsym,
    s(:str, "foo"),
    s(:begin,
      s(:lvar, "bar"))))
