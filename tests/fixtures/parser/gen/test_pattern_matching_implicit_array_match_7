--INPUT
case foo; in 1, "a", [], {} then nil; end
--LOCATIONS
~~~~ keyword ()
                                      ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
     ~~~ name (expr)
     ~~~ expression (expr)
                            ~~~~ begin (in_body[0])
          ~~ keyword (in_body[0])
          ~~~~~~~~~~~~~~~~~~~~~~~~~~ expression (in_body[0])
             ~~~~~~~~~~~~~~ expression (in_body[0]/pattern)
             ~ expression (in_body[0]/pattern/item[0])
                ~ begin (in_body[0]/pattern/item[1])
                  ~ end (in_body[0]/pattern/item[1])
                ~~~ expression (in_body[0]/pattern/item[1])
                     ~ begin (in_body[0]/pattern/item[2])
                      ~ end (in_body[0]/pattern/item[2])
                     ~~ expression (in_body[0]/pattern/item[2])
                         ~ begin (in_body[0]/pattern/item[3])
                          ~ end (in_body[0]/pattern/item[3])
                         ~~ expression (in_body[0]/pattern/item[3])
                                 ~~~ expression (in_body[0]/body)
--AST
s(:case_match,
  s(:lvar, "foo"),
  s(:in_pattern,
    s(:array_pattern,
      s(:int, "1"),
      s(:str, "a"),
      s(:array_pattern),
      s(:hash_pattern)), nil,
    s(:nil)), nil)
