--INPUT
a b(c(d)), :e do end
--LOCATIONS
              ~~ begin ()
                 ~~~ end ()
~~~~~~~~~~~~~~~~~~~~ expression ()
~ selector (call)
~~~~~~~~~~~~~ expression (call)
   ~ begin (call/arg[0])
        ~ end (call/arg[0])
  ~ selector (call/arg[0])
  ~~~~~~~ expression (call/arg[0])
     ~ begin (call/arg[0]/arg[0])
       ~ end (call/arg[0]/arg[0])
    ~ selector (call/arg[0]/arg[0])
    ~~~~ expression (call/arg[0]/arg[0])
      ~ selector (call/arg[0]/arg[0]/arg[0])
      ~ expression (call/arg[0]/arg[0]/arg[0])
           ~ begin (call/arg[1])
           ~~ expression (call/arg[1])
--AST
s(:block,
  s(:send, nil, "a",
    s(:send, nil, "b",
      s(:send, nil, "c",
        s(:send, nil, "d"))),
    s(:sym, "e")), nil, nil)
