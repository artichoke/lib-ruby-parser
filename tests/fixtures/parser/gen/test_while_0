--INPUT
while foo do meth end
--LOCATIONS
          ~~ begin ()
                  ~~~ end ()
~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~~~ expression ()
      ~~~ name (cond)
      ~~~ expression (cond)
             ~~~~ selector (body)
             ~~~~ expression (body)
--AST
s(:while,
  s(:lvar, "foo"),
  s(:send, nil, "meth"))
