--INPUT
while not (true) do end
--LOCATIONS
                 ~~ begin ()
                    ~~~ end ()
~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~~~~~ expression ()
      ~~~ selector (expr)
      ~~~~~~~~~~ expression (expr)
          ~ begin (expr/recv)
               ~ end (expr/recv)
          ~~~~~~ expression (expr/recv)
           ~~~~ expression (expr/recv/stmt[0])
--AST
s(:while,
  s(:send,
    s(:begin,
      s(:true)), "!"), nil)
