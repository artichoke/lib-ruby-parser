--INPUT
f{ |a, o=1, o1=2, *r, &b| }
--LOCATIONS
 ~ begin ()
                          ~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
~ selector (call)
~ expression (call)
   ~ begin (args)
                        ~ end (args)
   ~~~~~~~~~~~~~~~~~~~~~~ expression (args)
    ~ name (args/arg[0])
    ~ expression (args/arg[0])
        ~ operator (args/arg[1])
       ~ name (args/arg[1])
       ~~~ expression (args/arg[1])
         ~ expression (args/arg[1]/default)
              ~ operator (args/arg[2])
            ~~ name (args/arg[2])
            ~~~~ expression (args/arg[2])
               ~ expression (args/arg[2]/default)
                   ~ name (args/arg[3])
                  ~~ expression (args/arg[3])
                       ~ name (args/arg[4])
                      ~~ expression (args/arg[4])
--AST
s(:block,
  s(:send, nil, "f"),
  s(:args,
    s(:arg, "a"),
    s(:optarg, "o",
      s(:int, "1")),
    s(:optarg, "o1",
      s(:int, "2")),
    s(:restarg, "r"),
    s(:blockarg, "b")), nil)
