--INPUT
foo = bar, 1
--LOCATIONS
    ~ operator ()
~~~ name ()
~~~~~~~~~~~~ expression ()
      ~~~~~~ expression (value)
      ~~~ name (value/item[0])
      ~~~ expression (value/item[0])
           ~ expression (value/item[1])
--AST
s(:lvasgn, "foo",
  s(:array,
    s(:lvar, "bar"),
    s(:int, "1")))
