--INPUT
case foo; in x then x; end
--LOCATIONS
~~~~ keyword ()
                       ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
     ~~~ expression (expr)
               ~~~~ begin (in_body[0])
          ~~ keyword (in_body[0])
          ~~~~~~~~~~~ expression (in_body[0])
             ~ name (in_body[0]/pattern)
             ~ expression (in_body[0]/pattern)
                    ~ expression (in_body[0]/body)
--AST
s(:case_match,
  s(:lvar, "foo"),
  s(:in_pattern,
    s(:match_var, "x"), nil,
    s(:lvar, "x")), nil)
