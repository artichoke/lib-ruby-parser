--INPUT
        case [__FILE__, __LINE__ + 1, __ENCODING__]
          in [__FILE__, __LINE__, __ENCODING__]
        end

--AST
s(:case_match,
  s(:array,
    s(:str, "(assert_parses)"),
    s(:send,
      s(:int, "LINE"), "+",
      s(:int, "1")),
    s(:__ENCODING__)),
  s(:in_pattern,
    s(:array_pattern,
      s(:str, "(assert_parses)"),
      s(:int, "LINE"),
      s(:__ENCODING__)), nil, nil), nil)
