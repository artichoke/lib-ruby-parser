--INPUT
        case [__FILE__, __LINE__ + 1, __ENCODING__]
          in [__FILE__, __LINE__, __ENCODING__]
        end

--AST
s(:case_match,
  s(:array,
    s(:__FILE__),
    s(:send,
      s(:__LINE__), "+",
      s(:int, "1")),
    s(:__ENCODING__)),
  s(:in_pattern,
    s(:array_pattern,
      s(:__FILE__),
      s(:__LINE__),
      s(:__ENCODING__)), nil, nil), nil)
