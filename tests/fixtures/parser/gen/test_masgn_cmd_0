--INPUT
foo, bar = m foo
--LOCATIONS
         ~ operator ()
~~~~~~~~~~~~~~~~ expression ()
~~~~~~~~ expression (lhs)
~~~ name (lhs/item[0])
~~~ expression (lhs/item[0])
     ~~~ name (lhs/item[1])
     ~~~ expression (lhs/item[1])
           ~ selector (rhs)
           ~~~~~ expression (rhs)
             ~~~ name (rhs/args[0])
             ~~~ expression (rhs/args[0])
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "foo"),
    s(:lvasgn, "bar")),
  s(:send, nil, "m",
    s(:lvar, "foo")))
