--INPUT
foo[m bar]
--LOCATIONS
   ~ begin ()
         ~ end ()
~~~~~~~~~~ expression ()
~~~ name (recv)
~~~ expression (recv)
    ~ selector (index[0])
    ~~~~~ expression (index[0])
      ~~~ name (index[0]/args[0])
      ~~~ expression (index[0]/args[0])
--AST
s(:index,
  s(:lvar, "foo"),
  s(:send, nil, "m",
    s(:lvar, "bar")))
