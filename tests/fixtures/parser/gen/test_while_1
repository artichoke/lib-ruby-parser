--INPUT
while foo; meth end
--LOCATIONS
         ~ begin ()
                ~~~ end ()
~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~ expression ()
      ~~~ name (expr)
      ~~~ expression (expr)
           ~~~~ selector (body)
           ~~~~ expression (body)
--AST
s(:while,
  s(:lvar, "foo"),
  s(:send, nil, "meth"))
