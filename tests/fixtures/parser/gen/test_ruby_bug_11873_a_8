--INPUT
a b{c d}, 1.0 do end
--LOCATIONS
              ~~ begin ()
                 ~~~ end ()
~~~~~~~~~~~~~~~~~~~~ expression ()
~ selector (call)
~~~~~~~~~~~~~ expression (call)
   ~ begin (call/args[0])
       ~ end (call/args[0])
  ~~~~~~ expression (call/args[0])
  ~ selector (call/args[0]/call)
  ~ expression (call/args[0]/call)
    ~ selector (call/args[0]/body)
    ~~~ expression (call/args[0]/body)
      ~ selector (call/args[0]/body/args[0])
      ~ expression (call/args[0]/body/args[0])
          ~~~ expression (call/args[1])
--AST
s(:block,
  s(:send, nil, "a",
    s(:block,
      s(:send, nil, "b"), nil,
      s(:send, nil, "c",
        s(:send, nil, "d"))),
    s(:float, "1.0")), nil, nil)
