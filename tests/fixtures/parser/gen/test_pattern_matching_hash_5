--INPUT
case foo; in **a then true; end
--LOCATIONS
~~~~ keyword ()
                            ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
     ~~~ name (expr)
     ~~~ expression (expr)
                 ~~~~ begin (in_body[0])
          ~~ keyword (in_body[0])
          ~~~~~~~~~~~~~~~~ expression (in_body[0])
             ~~~ expression (in_body[0]/pattern)
             ~~ operator (in_body[0]/pattern/element[0])
             ~~~ expression (in_body[0]/pattern/element[0])
                      ~~~~ expression (in_body[0]/body)
--AST
s(:case_match,
  s(:lvar, "foo"),
  s(:in_pattern,
    s(:hash_pattern,
      s(:match_rest,
        s(:match_var, "a"))), nil,
    s(:true)), nil)
