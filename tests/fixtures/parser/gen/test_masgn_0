--INPUT
foo, bar = 1, 2
--LOCATIONS
         ~ operator ()
~~~~~~~~~~~~~~~ expression ()
~~~~~~~~ expression (lhs)
~~~ expression (lhs/item[0])
     ~~~ expression (lhs/item[1])
           ~~~~ expression (rhs)
           ~ expression (rhs/element[0])
              ~ expression (rhs/element[1])
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "foo"),
    s(:lvasgn, "bar")),
  s(:array,
    s(:int, "1"),
    s(:int, "2")))
