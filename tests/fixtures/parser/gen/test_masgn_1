--INPUT
(foo, bar) = 1, 2
--LOCATIONS
           ~ operator ()
~~~~~~~~~~~~~~~~~ expression ()
~ begin (lhs)
         ~ end (lhs)
~~~~~~~~~~ expression (lhs)
 ~~~ name (lhs/item[0])
 ~~~ expression (lhs/item[0])
      ~~~ name (lhs/item[1])
      ~~~ expression (lhs/item[1])
             ~~~~ expression (rhs)
             ~ expression (rhs/item[0])
                ~ expression (rhs/item[1])
--AST
s(:masgn,
  s(:mlhs,
    s(:lvasgn, "foo"),
    s(:lvasgn, "bar")),
  s(:array,
    s(:int, "1"),
    s(:int, "2")))
