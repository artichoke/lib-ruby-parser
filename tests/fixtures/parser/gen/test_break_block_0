--INPUT
break fun foo do end
--LOCATIONS
~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~~ expression ()
              ~~ begin (value)
                 ~~~ end (value)
      ~~~~~~~~~~~~~~ expression (value)
      ~~~ selector (value/call)
      ~~~~~~~ expression (value/call)
          ~~~ name (value/call/args[0])
          ~~~ expression (value/call/args[0])
--AST
s(:break,
  s(:block,
    s(:send, nil, "fun",
      s(:lvar, "foo")), nil, nil))
