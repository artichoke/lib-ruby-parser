--INPUT
break fun foo do end
--LOCATIONS
~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~~ expression ()
              ~~ begin (arg[0])
                 ~~~ end (arg[0])
      ~~~~~~~~~~~~~~ expression (arg[0])
      ~~~ selector (arg[0]/call)
      ~~~~~~~ expression (arg[0]/call)
          ~~~ name (arg[0]/call/arg[0])
          ~~~ expression (arg[0]/call/arg[0])
--AST
s(:break,
  s(:block,
    s(:send, nil, "fun",
      s(:lvar, "foo")), nil, nil))
