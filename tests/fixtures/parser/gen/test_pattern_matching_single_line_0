--INPUT
1 in [a]; a
--LOCATIONS
~~~~~~~~~~~ expression ()
  ~~ operator (stmt[0])
~~~~~~~~ expression (stmt[0])
~ expression (stmt[0]/lhs)
     ~ begin (stmt[0]/rhs)
       ~ end (stmt[0]/rhs)
     ~~~ expression (stmt[0]/rhs)
      ~ name (stmt[0]/rhs/item[0])
      ~ expression (stmt[0]/rhs/item[0])
          ~ name (stmt[1])
          ~ expression (stmt[1])
--AST
s(:begin,
  s(:in_match,
    s(:int, "1"),
    s(:array_pattern,
      s(:match_var, "a"))),
  s(:lvar, "a"))
