--INPUT
meth 1 do end.fun(bar) {}
--LOCATIONS
                       ~ begin ()
                        ~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
                 ~ begin (call)
                     ~ end (call)
              ~~~ selector (call)
~~~~~~~~~~~~~~~~~~~~~~ expression (call)
       ~~ begin (call/recv)
          ~~~ end (call/recv)
~~~~~~~~~~~~~ expression (call/recv)
~~~~ selector (call/recv/call)
~~~~~~ expression (call/recv/call)
     ~ expression (call/recv/call/args[0])
                  ~~~ name (call/args[0])
                  ~~~ expression (call/args[0])
--AST
s(:block,
  s(:send,
    s(:block,
      s(:send, nil, "meth",
        s(:int, "1")), nil, nil), "fun",
    s(:lvar, "bar")), nil, nil)
