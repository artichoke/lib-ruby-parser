--INPUT
def f a, &b; end
--LOCATIONS
~~~ keyword ()
    ~ name ()
             ~~~ end ()
~~~~~~~~~~~~~~~~ expression ()
      ~~~~~ expression (args)
      ~ name (args/arg[0])
      ~ expression (args/arg[0])
          ~ name (args/arg[1])
         ~~ expression (args/arg[1])
--AST
s(:def, "f",
  s(:args,
    s(:arg, "a"),
    s(:blockarg, "b")), nil)
