--INPUT
while class << self; a = tap do end; end; break; end
--LOCATIONS
                                        ~ begin ()
                                                 ~~~ end ()
~~~~~ keyword ()
~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
      ~~~~~ keyword (expr)
            ~~ operator (expr)
                                     ~~~ end (expr)
      ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ expression (expr)
               ~~~~ expression (expr/of)
                       ~ operator (expr/body)
                     ~ name (expr/body)
                     ~~~~~~~~~~~~~~ expression (expr/body)
                             ~~ begin (expr/body/value)
                                ~~~ end (expr/body/value)
                         ~~~~~~~~~~ expression (expr/body/value)
                         ~~~ selector (expr/body/value/call)
                         ~~~ expression (expr/body/value/call)
                                          ~~~~~ keyword (body)
                                          ~~~~~ expression (body)
--AST
s(:while,
  s(:sclass,
    s(:self),
    s(:lvasgn, "a",
      s(:block,
        s(:send, nil, "tap"), nil, nil))),
  s(:break))
