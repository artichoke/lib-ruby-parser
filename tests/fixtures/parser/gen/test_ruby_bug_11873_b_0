--INPUT
p p{p(p);p p}, tap do end
--LOCATIONS
                   ~~ begin ()
                      ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~~~~~ expression ()
~ selector (call)
~~~~~~~~~~~~~~~~~~ expression (call)
   ~ begin (call/args[0])
            ~ end (call/args[0])
  ~~~~~~~~~~~ expression (call/args[0])
  ~ selector (call/args[0]/call)
  ~ expression (call/args[0]/call)
    ~~~~~~~~ expression (call/args[0]/body)
     ~ begin (call/args[0]/body/stmt[0])
       ~ end (call/args[0]/body/stmt[0])
    ~ selector (call/args[0]/body/stmt[0])
    ~~~~ expression (call/args[0]/body/stmt[0])
      ~ selector (call/args[0]/body/stmt[0]/args[0])
      ~ expression (call/args[0]/body/stmt[0]/args[0])
         ~ selector (call/args[0]/body/stmt[1])
         ~~~ expression (call/args[0]/body/stmt[1])
           ~ selector (call/args[0]/body/stmt[1]/args[0])
           ~ expression (call/args[0]/body/stmt[1]/args[0])
               ~~~ selector (call/args[1])
               ~~~ expression (call/args[1])
--AST
s(:block,
  s(:send, nil, "p",
    s(:block,
      s(:send, nil, "p"), nil,
      s(:begin,
        s(:send, nil, "p",
          s(:send, nil, "p")),
        s(:send, nil, "p",
          s(:send, nil, "p")))),
    s(:send, nil, "tap")), nil, nil)
