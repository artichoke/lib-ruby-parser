--INPUT
f(a ? "a":1)
--LOCATIONS
 ~ begin ()
           ~ end ()
~ selector ()
~~~~~~~~~~~~ expression ()
         ~ colon (args[0])
    ~ question (args[0])
  ~~~~~~~~~ expression (args[0])
  ~ selector (args[0]/cond)
  ~ expression (args[0]/cond)
      ~ begin (args[0]/truthy)
        ~ end (args[0]/truthy)
      ~~~ expression (args[0]/truthy)
          ~ expression (args[0]/falsey)
--AST
s(:send, nil, "f",
  s(:if,
    s(:send, nil, "a"),
    s(:str, "a"),
    s(:int, "1")))
