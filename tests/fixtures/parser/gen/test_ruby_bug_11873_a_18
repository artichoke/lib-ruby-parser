--INPUT
a b(c d), 1.0i do end
--LOCATIONS
               ~~ begin ()
                  ~~~ end ()
~~~~~~~~~~~~~~~~~~~~~ expression ()
~ selector (call)
~~~~~~~~~~~~~~ expression (call)
   ~ begin (call/args[0])
       ~ end (call/args[0])
  ~ selector (call/args[0])
  ~~~~~~ expression (call/args[0])
    ~ selector (call/args[0]/args[0])
    ~~~ expression (call/args[0]/args[0])
      ~ selector (call/args[0]/args[0]/args[0])
      ~ expression (call/args[0]/args[0]/args[0])
          ~~~~ expression (call/args[1])
--AST
s(:block,
  s(:send, nil, "a",
    s(:send, nil, "b",
      s(:send, nil, "c",
        s(:send, nil, "d"))),
    s(:complex, "1.0i")), nil, nil)
