--INPUT
foo::a += m foo
--LOCATIONS
       ~~ operator (root)
~~~~~~~~~~~~~~~ expression (root)
     ~ selector (root -> recv)
~~~~~~ expression (root -> recv)
~~~ expression (root -> recv -> recv)
--AST
s(:op_asgn,
  s(:send,
    s(:lvar, "foo"), "a"), "+",
  s(:send, nil, "m",
    s(:lvar, "foo")))
