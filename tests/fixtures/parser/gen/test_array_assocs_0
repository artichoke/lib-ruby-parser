--INPUT
[ 1 => 2 ]
--LOCATIONS
~ begin (root)
         ~ end (root)
~~~~~~~~~~ expression (root)
  ~~~~~~ expression (root -> elements -> 0)
    ~~ operator (root -> elements -> 0 -> pairs -> 0)
  ~~~~~~ expression (root -> elements -> 0 -> pairs -> 0)
  ~ expression (root -> elements -> 0 -> pairs -> 0 -> key)
       ~ expression (root -> elements -> 0 -> pairs -> 0 -> value)
--AST
s(:array,
  s(:hash,
    s(:pair,
      s(:int, "1"),
      s(:int, "2"))))
