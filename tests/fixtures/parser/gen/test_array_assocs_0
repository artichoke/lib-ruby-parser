--INPUT
[ 1 => 2 ]
--LOCATIONS
~ begin ()
         ~ end ()
~~~~~~~~~~ expression ()
  ~~~~~~ expression (item[0])
    ~~ operator (item[0]/pair[0])
  ~~~~~~ expression (item[0]/pair[0])
  ~ expression (item[0]/pair[0]/key)
       ~ expression (item[0]/pair[0]/value)
--AST
s(:array,
  s(:hash,
    s(:pair,
      s(:int, "1"),
      s(:int, "2"))))
