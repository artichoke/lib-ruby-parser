--INPUT
foo[0, 1] ||= 2
--LOCATIONS
          ~~~ operator ()
~~~~~~~~~~~~~~~ expression ()
   ~ begin (recv)
        ~ end (recv)
~~~~~~~~~ expression (recv)
~~~ name (recv/recv)
~~~ expression (recv/recv)
    ~ expression (recv/index[0])
       ~ expression (recv/index[1])
              ~ expression (value)
--AST
s(:or_asgn,
  s(:indexasgn,
    s(:lvar, "foo"),
    s(:int, "0"),
    s(:int, "1")),
  s(:int, "2"))
