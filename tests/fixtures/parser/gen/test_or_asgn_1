--INPUT
foo[0, 1] ||= 2
--LOCATIONS
          ~~~ operator (root)
~~~~~~~~~~~~~~~ expression (root)
   ~ begin (root -> recv)
        ~ end (root -> recv)
~~~~~~~~~ expression (root -> recv)
~~~ expression (root -> recv -> recv)
    ~ expression (root -> recv -> indexes -> 0)
       ~ expression (root -> recv -> indexes -> 1)
              ~ expression (root -> value)
--AST
s(:or_asgn,
  s(:indexasgn,
    s(:lvar, "foo"),
    s(:int, "0"),
    s(:int, "1")),
  s(:int, "2"))
