--INPUT
"foo#{bar}baz"
--LOCATIONS
~ begin ()
             ~ end ()
~~~~~~~~~~~~~~ expression ()
 ~~~ expression (part[0])
    ~~ begin (part[1])
         ~ end (part[1])
    ~~~~~~ expression (part[1])
      ~~~ expression (part[1]/stmt[0])
          ~~~ expression (part[2])
--AST
s(:dstr,
  s(:str, "foo"),
  s(:begin,
    s(:lvar, "bar")),
  s(:str, "baz"))
