--INPUT
%W\blah #x a #@a b #$b c #{3} # \
--TOKENS
tWORDS_BEG "%W\\" [0, 3]
tSTRING_CONTENT "blah" [3, 7]
tSPACE "" [7, 8]
tSTRING_CONTENT "#x" [8, 10]
tSPACE "" [10, 11]
tSTRING_CONTENT "a" [11, 12]
tSPACE "" [12, 13]
tSTRING_DVAR "" [13, 14]
tIVAR "@a" [14, 16]
tSPACE "" [16, 17]
tSTRING_CONTENT "b" [17, 18]
tSPACE "" [18, 19]
tSTRING_DVAR "" [19, 20]
tGVAR "$b" [20, 22]
tSPACE "" [22, 23]
tSTRING_CONTENT "c" [23, 24]
tSPACE "" [24, 25]
tSTRING_DBEG "\#{" [25, 27]
tINTEGER "3" [27, 28]
tRCURLY "}" [28, 29]
tSPACE "" [29, 30]
tSTRING_CONTENT "#" [30, 31]
tSPACE "" [31, 32]
tSTRING_END "\\" [32, 33]
