--INPUT
a = <<"EOF"
#x a #@a b #$b c #{3} 
EOF

--TOKENS
tIDENTIFIER "a" [0, 1]
tEQL "=" [2, 3]
tSTRING_BEG "<<\"" [4, 11]
tSTRING_CONTENT "#x a " [12, 17]
tSTRING_DVAR "" [17, 18]
tIVAR "@a" [18, 20]
tSTRING_CONTENT " b " [20, 23]
tSTRING_DVAR "" [23, 24]
tGVAR "$b" [24, 26]
tSTRING_CONTENT " c " [26, 29]
tSTRING_DBEG "\#{" [29, 31]
tINTEGER "3" [31, 32]
tRCURLY "}" [32, 33]
tSTRING_CONTENT " \n" [33, 35]
tSTRING_END "EOF" [35, 38]
tNL "\n" [11, 12]
